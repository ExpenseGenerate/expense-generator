<h1>Expenses</h1>
<form action='/calculate' method='get'>
  <input class='submit' type='submit' value='Go Back'/>
</form>
<table>
  <tr class ="headings">
    <td>Item</td>
    <td>Daily</td>
    <td>Weekly</td>
    <td>Monthly</td>
    <td>Yearly</td>
  </tr>
  {{#each data}}
  <tr>
    <td>{{item}}</td>
    <td>${{pricePerDay}}</td>
    <td>${{pricePerWeek}}</td>
    <td>${{pricePerMonth}} </td>
    <td>${{pricePerYear}}</td>
  </tr>
  {{/each}}
</table>
